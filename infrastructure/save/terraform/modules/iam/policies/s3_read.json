{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "ListModelBuckets",
      "Effect": "Allow",
      "Action": ["s3:ListBucket"],
      "Resource": [
        "arn:aws:s3:::vllm-platform-*"
      ]
    },
    {
      "Sid": "ReadModelObjects",
      "Effect": "Allow",
      "Action": ["s3:GetObject"],
      "Resource": [
        "arn:aws:s3:::vllm-platform-*/*"
      ]
    }
  ]
}
